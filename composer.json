{
    "name": "liip/url-auto-converter-bundle",
    "type": "symfony-bundle",
    "description": "This bundle adds a Twig Extension with a filter for automatically converting urls and emails in a string to html links",
    "homepage": "http://liip.ch",
    "keywords": ["url"],
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Liip AG",
            "homepage": "http://www.liip.ch/"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/liip/LiipUrlAutoConverterBundle/contributors"
        }
    ],
    "require": {
        "php": "^5.3.9|^7.0",
        "symfony/framework-bundle": "~2.3|~3.0",
        "twig/twig": "~1.12|~2.0"
    },
    "require-dev": {
        "symfony/symfony": "~2.3|~3.0"
    },
    "autoload": {
        "psr-4": { "Liip\\UrlAutoConverterBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" },
        "classmap": [ "Tests/phpunit-bc" ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3.x-dev"
        }
    }
}
